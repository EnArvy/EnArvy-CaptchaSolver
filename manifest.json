{
    "name": "EnArvy's Captcha Solver",
    "description": "Chrome extension to solve numerous captchas",
    "version": "1.0",
    "manifest_version": 3,
    "web_accessible_resources": [{
      "resources":[
      "js/worker.min.js",
      "js/tesseract-core.wasm.js",
      "traineddata/*.traineddata.gz",
      "js/tesseract.min.js","marvin.min.js"],"matches":["https://webmail.iitd.ac.in/*"]}
    ],
    "content_scripts":
      [
        {
          "matches": ["https://moodle.iitd.ac.in/login/index.php"],
          "js":["js/moodle.js"]
        },
        {
          "matches":["https://webmail.iitd.ac.in/*"],
          "js":["js/marvin.min.js","js/tesseract/tesseract.min.js","js/webmail.js"]
        }
      ],
    "permissions": ["storage"],
    "action": {
      "default_popup": "popup.html"
    },
    
    "options_ui":{
      "page":"options.html",
      "open_in_tab":true
    }
    
  }